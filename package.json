    "graphql-depth-limit": "^1.1.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-upload": "^17.0.0",
    "graphql-upload-ts": "^2.1.2",
    "graphql-ws": "^6.0.4",
    "keyv": "^5.2.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "subscriptions-transport-ws": "^0.11.0",
